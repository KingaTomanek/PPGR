#include <iostream>
using namespace std;

void Fibonacci(int n);

int main() {
    int n;
    cout << "Podaj liczbÄ™ Fibonacciego: ";
    cin >> n;
    
    Fibonacci(n);

    return 0;
}

void Fibonacci(int n) {
    long long x = 0, y = 1;

    for (int i = 0; i < n; i++) {
        cout << x << " ";
        long long next = x + y;
        y = x;
        x = next;
    }
}
